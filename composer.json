{
  "name": "invoicetic/fgoro",
  "type": "library",
  "license": "MIT",
  "description": "invoice Fgoro gateway",
  "keywords": [],
  "homepage": "https://github.com/invoicetic/fgoro",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Invoicetic\\Fgoro\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Invoicetic\\Fgoro\\Tests\\": "tests/src",
      "Invoicetic\\Fgoro\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "invoicetic/common": "dev-main",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "symfony/http-client": "^6.3",
    "invoicetic/invoicetic": "dev-main",
    "symfony/dotenv": "^6.0|^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true,
      "php-http/discovery": true
    }
  }
}
